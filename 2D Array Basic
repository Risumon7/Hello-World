//2D Array declaration, direct initialization and printing
#include<stdio.h>
int main()
    {
        int i, j;
        int A[3][4] = { {2,3,4,5}, {22,34,54,65}, {43,21,43,65} };
        for(i=0; i<3; i++) {
            for(j=0; j<4; j++) {
                printf("%d ", A[i][j]);
            }
            printf("\n");
        }
        return 0;
    }
