// Array: max num finding
#include <stdio.h>

int main()
{
    int num[100];
    int max, i, n;
    printf("How many numbers?\n");
    scanf("%d", &n);
    for(i=0; i<n; i++) {
        scanf("%d", &num[i]);
    }
    max = num[0];
    for(i=0; i<n; i++) {
        if(max < num[i]) {
            max = num[i];
        }
    }
    
    printf("The max is: %d\n", max);
    return 0;
}
