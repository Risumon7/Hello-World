//copying one array value to another
#include<stdio.h>
int main()
    {
        int array1[30], array2[30],i,n;
        printf("How many numbers?\n");
        scanf("%d", &n);
        for(i=0; i<n; i++) {
                scanf("%d", &array1[i]);
        }
        printf("array1:\n");
        for(i=0; i<n; i++) {
            printf("%d ",array1[i]);
        }
        for(i=0; i<n; i++) {
            array2[i] = array1[i];
        }
        printf("\narray2:\n");
        for(i=0; i<n; i++) {
            printf("%d ", array2[i]);
        }
        return 0;
    }
