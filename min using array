// Array sum and avg 
#include <stdio.h>

int main()
{
    int num[100];
    int min, i, n;
    printf("How many numbers?\n");
    scanf("%d", &n);
    for(i=0; i<n; i++) {
        scanf("%d", &num[i]);
    }
    min = num[0];
    for(i=0; i<n; i++) {
        if(min > num[i]) {
            min = num[i];
        }
    }
    
    printf("The min is: %d\n", min);
    return 0;
}
